generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Session {
  accessToken String @unique
  tokenType   String
  expiresIn   Int
  user User @relation(fields: [userId], references: [id]) 
  userId Int @unique
}

model User {
  id       Int       @id
  name     String
  lastname String
  email    String    @unique
  hash     String
  level    Int?
  session Session?

  @@map("user")
}

model Business {
  id Int @id
  publicId String 
  name String
  @@map("business")
}